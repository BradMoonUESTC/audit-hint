{
  "name": "vscode-rag-vulnerability-detector",
  "displayName": "代码漏洞检测器",
  "description": "使用RAG技术检测代码中的潜在安全漏洞",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vulnerability-detector.openVulnerabilityPanel",
        "title": "打开代码漏洞分析面板"
      },
      {
        "command": "vulnerability-analyzer.analyzeSelectedCode",
        "title": "分析此段代码的checklist"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "vulnerability-analyzer.analyzeSelectedCode",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vulnerability-detector.openVulnerabilityPanel",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.1"
  }
}
