{
  "name": "vscode-rag-vulnerability-hinter",
  "displayName": "Code Audit Hinter",
  "description": "hinter for your audit and development",
  "version": "0.1.5",
  "publisher": "BradMoonUESTC",
  "repository": {
    "type": "git",
    "url": "https://github.com/BradMoonUESTC/audit-hint.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#C80000",
    "theme": "dark"
  },
  "keywords": [
    "安全",
    "漏洞",
    "代码审计",
    "RAG",
    "AI",
    "audit hint",
    "security",
    "vulnerability"
  ],
  "license": "MIT",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vulnerability-detector.openVulnerabilityPanel",
        "title": "打开代码漏洞checklist分析面板"
      },
      {
        "command": "vulnerability-analyzer.analyzeSelectedCode",
        "title": "分析此段代码的checklist"
      },
      {
        "command": "vulnerability-detector.openConfiguration",
        "title": "配置代码审计助手API密钥"
      },
      {
        "command": "vulnerability-detector.generateTestCase",
        "title": "生成测试用例"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "vulnerability-analyzer.analyzeSelectedCode",
          "group": "navigation"
        },
        {
          "when": "editorHasSelection",
          "command": "vulnerability-detector.generateTestCase",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "vulnerability-detector.openVulnerabilityPanel",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v"
      }
    ],
    "configuration": {
      "title": "代码审计助手",
      "properties": {
        "codeAuditHinter.openaiApiBase": {
          "type": "string",
          "default": "4.0.wokaai.com",
          "description": "OpenAI API 基础URL"
        },
        "codeAuditHinter.openaiApiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API 密钥"
        },
        "codeAuditHinter.pineconeApiKey": {
          "type": "string",
          "default": "",
          "description": "Pinecone API 密钥"
        },
        "codeAuditHinter.pineconeHost": {
          "type": "string",
          "default": "",
          "description": "Pinecone 主机地址"
        },
        "codeAuditHinter.pineconeNamespace": {
          "type": "string",
          "default": "vulns_high",
          "description": "Pinecone 命名空间"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1"
  },
  "author": "",
  "dependencies": {
    "axios": "^1.8.1",
    "dotenv": "^16.4.7",
    "vsce": "^2.15.0"
  }
}
